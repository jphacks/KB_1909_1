{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/axios",
      "vue-router",
      "vuetify"
    ],
    "paths": { "@/*": ["./*"], "~/*": ["./*"] },
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "include": ["*"]
}
